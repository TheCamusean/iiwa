<?xml version="1.0"?>

<robot name="schunk_adapter" xmlns:xacro="http://www.ros.org/wiki/xacro">

  <xacro:macro name="schunk_adapter_gazebo" params="name">

    <gazebo reference="${name}_link">
      <material>Gazebo/DarkGrey</material>
      <mu1>0.2</mu1>
      <mu2>0.2</mu2>
    </gazebo>

  </xacro:macro>

</robot>

